<% filtered_env = @request.filtered_env -%>

<ul style="list-style: none">
  <% filtered_env.keys.map(&:to_s).sort.each do |key| -%>
    <li>
      <strong><%= key %>:</strong>
      <%= filtered_env[key].nil? ? "nil" : render(partial: "value", object: filtered_env[key]) %>
    </li>
  <% end -%>
</ul>
